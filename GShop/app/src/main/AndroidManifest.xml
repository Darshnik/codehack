<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gshop.schatu2.gshop">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".RecyclerViewActivity"></activity>

        <activity android:name=".AccountActivity"></activity>

        <activity android:name=".OrderActivity"></activity>

        <activity android:name=".WalletActivity"></activity>

        <activity android:name=".ProductLayout"></activity>

        <activity android:name=".Checkout"></activity>
    </application>

</manifest>
